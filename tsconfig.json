{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "lib": ["es2017", "dom"],
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "baseUrl": "./",
      "paths": {
        "ol": ["node_modules/ol/src"],
        "ol/*": ["node_modules/ol/src/*"]
      }
    },
    "include": [
      "src/**/*.js",
      "node_modules/ol/**/*.js"
    ]
}
